{
  "name": "webrobot-etl-api-doc",
  "version": "1.0.0",
  "description": "WebRobot ETL API Documentation",
  "scripts": {
    "start": "redocly preview --product realm",
    "build": "redocly build-docs",
    "build-docs": "redocly build-docs",
    "postinstall": "node scripts/disable-search.js && node scripts/patch-otel.js",
    "docker:build": "docker build -t webrobot-etl-api-doc .",
    "docker:build-docs": "docker build -f Dockerfile.docs -t webrobot-etl-api-doc:docs .",
    "docker:run": "docker run -p 8080:80 webrobot-etl-api-doc",
    "docker:run-docs": "docker run -p 8080:80 webrobot-etl-api-doc:docs",
    "lint": "redocly lint openapi.yaml",
    "bundle": "redoc-cli bundle openapi.yaml -o dist/index.html"
  },
  "keywords": [
    "api",
    "documentation",
    "openapi",
    "redocly"
  ],
  "author": "WebRobot",
  "license": "PROPRIETARY",
  "devDependencies": {
    "@redocly/cli": "^2.12.6",
    "@redocly/realm": "^0.129.0-next.0",
    "redoc-cli": "^0.13.21"
  },
  "overrides": {
    "@opentelemetry/resources": "^1.30.1"
  }
}
